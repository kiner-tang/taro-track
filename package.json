{
  "name": "@kiner/track",
  "version": "0.0.1",
  "description": "基于taro的小程序数据埋点工具",
  "main": "src/entry/index.ts",
  "scripts": {
    "gulp:build": "gulp build",
    "publish:all": "bash ./tools/npm/publish.sh",
    "build:common": "gulp common",
    "publish:common": "cd dist/common;npm publish;",
    "build:core": "gulp core",
    "publish:core": "cd dist/core;npm publish;",
    "build:datasource": "gulp datasource",
    "publish:datasource": "cd dist/datasource;npm publish;",
    "build:transporter": "gulp transporter",
    "publish:transporter": "cd dist/transporter;npm publish;",
    "build:entry": "rm -rf ./dist/entry;gulp entry",
    "publish:entry": "cd dist/entry;npm publish;",
    "dev:es": "cross-env NODE_ENV=development webpack --config ./build/webpack.config.es6.js --watch",
    "build:es": "rm -rf ./dist/es;cross-env NODE_ENV=production webpack --config ./build/webpack.config.es6.js",
    "publish:es": "cd dist/es;npm publish;",
    "dev:plugins": "cross-env NODE_ENV=development webpack --config ./build/webpack.config.common.js --watch",
    "build:plugins": "rm -rf ./dist/plugins;cross-env NODE_ENV=production webpack --config ./build/webpack.config.common.js",
    "publish:plugins": "cd dist/plugins;npm publish;"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kiner-tang/taro-track"
  },
  "keywords": [
    "taro",
    "userAction",
    "track"
  ],
  "author": "kiner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kiner-tang/taro-track/issues"
  },
  "homepage": "https://github.com/kiner-tang/taro-track#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "@babel/runtime-corejs3": "^7.9.2",
    "@purtuga/esm-webpack-plugin": "^1.2.1",
    "@types/colors": "^1.2.1",
    "@types/fancy-log": "^1.3.1",
    "@types/fs-extra": "^8.1.0",
    "@types/gulp": "4.0.5",
    "@types/gzip-js": "^0.3.1",
    "@types/node": "^13.13.2",
    "@types/shelljs": "^0.8.8",
    "@types/uglify-js": "^3.9.0",
    "@types/xml2js": "^0.4.5",
    "@types/xml2json": "^0.11.0",
    "babel-loader": "^8.1.0",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.2",
    "fancy-log": "1.3.3",
    "fs": "^0.0.1-security",
    "fs-extra": "^9.0.0",
    "gulp": "4.0.2",
    "gulp-sourcemaps": "2.6.5",
    "gulp-ts-alias": "1.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gzip-js": "^0.3.2",
    "shelljs": "^0.8.4",
    "ts-loader": "^7.0.1",
    "ts-node": "8.5.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3",
    "uglify-js": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "wxml2json": "^2.0.1"
  },
  "dependencies": {
    "@kiner/taro-track-common": "^0.0.1",
    "@kiner/taro-track-corejs": "0.0.3",
    "@kiner/taro-track-datasource": "^0.0.3",
    "@kiner/taro-track-entrypoint": "0.0.4",
    "@kiner/taro-track-plugins": "0.0.7",
    "@kiner/taro-track-transporter": "0.0.2"
  }
}
